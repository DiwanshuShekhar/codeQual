p02761 s218895980
N, M = map(int, input().split())
sc = [list(map(int, input().split())) for _ in range(M)]

for i in range(1000):
    num = str(i)
    if len(num) != N:
        continue
    if all(num[s - 1] == str(c) for s, c in sc):
        print(i)
        break
else:
    print(-1)
