S = input()
A = ['AKIHABAR', 'KIHABAR', 'AKIHABR', 'KIHR', 'AKIHAB', 'AKIHAR', 'KIHAB', 'KIHAR']
for a in A:
    if S == a + 'A' * (9 - len(a)):
        print('YES')
        exit()
print('NO')
